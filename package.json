{
  "name": "pap-heartbeat-collector",
  "version": "1.0.0",
  "description": "Local heartbeat collector for PAP agents - collects heartbeats per cluster and alerts central on problems",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pap",
    "heartbeat",
    "collector",
    "agent",
    "monitoring"
  ],
  "author": "Plugged.in",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "@types/ws": "^8.5.13",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
